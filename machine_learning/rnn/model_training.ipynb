   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<_MapDataset element_spec=(TensorSpec(shape=(None, 48, 9), dtype=tf.float64, name=None), TensorSpec(shape=(None, 24), dtype=tf.float64, name=None))>"
      ]
     },
     "execution_count": 42,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "train_ds = tf.keras.utils.timeseries_dataset_from_array(\n",
    "    df_train.to_numpy(),\n",
